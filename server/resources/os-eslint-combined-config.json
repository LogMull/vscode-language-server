{
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "script"
  },
  "env": {
    "es2021": true,
    "jquery": true,
    "browser": true
  },
  "globals": {
    "document": "readonly",
    "navigator": "readonly",
    "window": "readonly",
    "zen": "readable",
    "zenIndex": "readable",
    "zenGetProp": "readable",
    "zenSetProp": "readable",
    "zenText": "readable",
    "zenASSERT": "readable",
    "zenExceptionHandler": "readable",
    "zenStackTrace": "readable",
    "zenIsMissing": "readable",
    "zenAttr": "readable",
    "zenTrim": "readable",
    "zenEscapeHTML": "readable",
    "zenEscapeJS": "readable",
    "zenBool": "readable",
    "zenConvertType": "readable",
    "zenGetComponentById": "readable",
    "zenGetComponent": "readable",
    "zenSetDeferredAction": "readable",
    "zenDeferredActionHandler": "readable",
    "zenFocus": "readable",
    "zenRunMethod": "readable",
    "cspRunServerMethodError": "readable",
    "zenCreate": "readable",
    "zenLoadClass": "readable",
    "zenStoreCookie": "readable",
    "zenLoadCookie": "readable",
    "zenOutputWindow": "readable",
    "zenTRACE": "readable",
    "zenSetUnloadInProgress": "readable",
    "zenClearUnloadInProgress": "readable",
    "zenPageEventHandler": "readable",
    "zenInitWindow": "readable",
    "zenGet": "readable",
    "zenJustify": "readable",
    "zenIsComponent": "readable",
    "zenGetLeft": "readable",
    "zenGetTop": "readable",
    "zenGetTopScroll": "readable",
    "zenGetLeftScroll": "readable",
    "zenGetPageScrollLeft": "readable",
    "zenGetPageScrollTop": "readable",
    "zenGetWindowWidth": "readable",
    "zenGetWindowHeight": "readable",
    "zenGetPageOffsets": "readable",
    "zenScrollIntoView": "readable",
    "zenAlert": "readable",
    "zenGetThreadId": "readable",
    "zenBuildArgumentList": "readable",
    "zenSerialState": "readable",
    "zenSerialState_getHeader": "readable",
    "zenSerialState_getBody": "readable",
    "zenSerialState_addObject": "readable",
    "zenSerialState_addZenProxy": "readable",
    "zenSerialState_serializeArray": "readable",
    "zenSerialState_serializeList": "readable",
    "zenListToArray": "readable",
    "zenArrayToList": "readable",
    "zenBeginDeferredRefresh": "readable",
    "zenEndDeferredRefresh": "readable",
    "zenRenderContents": "readable",
    "zenLaunchPopupWindow": "readable",
    "zenDialog_onStartModalHandler": "readable",
    "zenDialog_onEndModalHandler": "readable",
    "zenDialog_onCanEndModalHandler": "readable",
    "zenPopupClosed": "readable",
    "zenGetRootZenWindow": "readable",
    "zenInvokeCallbackMethod": "readable",
    "zenFireEvent": "readable",
    "zenParseDate": "readable",
    "zenDateToString": "readable",
    "zenStringToDate": "readable",
    "zenNormalizeTime": "readable",
    "zenUnescapeXML": "readable",
    "zenEscapeXML": "readable",
    "zenFormatDate": "readable",
    "zenFormatNumber": "readable",
    "zenApplyFormatSpec": "readable",
    "zenGetBBox": "readable",
    "zenGetComputedTextLength": "readable",
    "zenXMLParser": "readable",
    "zenXMLParser_parseString": "readable",
    "zenXMLParser_parse": "readable",
    "zenXMLParser_parseNode": "readable",
    "zenXMLContentHandler": "readable",
    "zenXMLContentHandler_startDocument": "readable",
    "zenXMLContentHandler_endDocument": "readable",
    "zenXMLContentHandler_startElement": "readable",
    "zenXMLContentHandler_endElement": "readable",
    "zenXMLContentHandler_characters": "readable",
    "zenDragData": "readable",
    "zenDragData_toString": "readable",
    "zenProxy": "readable",
    "zenProxy_clear": "readable",
    "zenProxy_serialize": "readable",
    "zenProxy_deserialize": "readable",
    "zenProxy_toString": "readable",
    "zenCreateComponent": "readable",
    "zenLoadJS": "readable",
    "zenLoadCSS": "readable",
    "zenUtilsVersion": "readable",
    "zenIsIE": "readable",
    "zenIsGecko": "readable",
    "zenIsChrome": "readable",
    "zenIsSafari": "readable",
    "zenIsSafariMobile": "readable",
    "zenDEBUGSerialize": "readable",
    "zenDEBUGTraceEvents": "readable",
    "zenShowClassError": "readable",
    "zenUserExceptionHandler": "readable",
    "_zenClassIdx": "readable",
    "_zenLoaderIdx": "readable",
    "_zenIndex": "readable",
    "_zenIds": "readable",
    "zenMaster": "readable",
    "zenPage": "readable",
    "_zenProxyIndex": "readable",
    "_zenProxyId": "readable",
    "zenProgressBarTimeout": "readable",
    "zenDeferredTimerId": "readable",
    "zenDeferredAction": "readable",
    "zenTrapTAB": "readable",
    "zenModalBackground": "readable",
    "zenBACKSPACE": "readable",
    "zenDELETE": "readable",
    "zenDOWN": "readable",
    "zenESC": "readable",
    "zenEND": "readable",
    "zenENTER": "readable",
    "zenF1": "readable",
    "zenF2": "readable",
    "zenF3": "readable",
    "zenF4": "readable",
    "zenF5": "readable",
    "zenF6": "readable",
    "zenF7": "readable",
    "zenF8": "readable",
    "zenF9": "readable",
    "zenF10": "readable",
    "zenF11": "readable",
    "zenF12": "readable",
    "zenHOME": "readable",
    "zenINSERT": "readable",
    "zenLEFT": "readable",
    "zenPAGEDN": "readable",
    "zenPAGEUP": "readable",
    "zenRIGHT": "readable",
    "zenSPACE": "readable",
    "zenTAB": "readable",
    "zenUP": "readable",
    "zenNotificationList": "readable",
    "zenUnloadInProgress": "readable",
    "zenSynchronousMode": "writeable",
    "zenSyncFlag": "readable",
    "zenRefreshList": "readable",
    "zenRenderList": "readable",
    "zenRefreshMode": "readable",
    "zenThreadId": "readable",
    "zenDeferStack": "readable",
    "zenThis": "readable",
    "zenEvent": "writeable",
    "zenInstanceMethod": "readable",
    "zenLink": "readable",
    "osCommand": "readable",
    "osCommandResponse": "readable",
    "osEvent": "readable",
    "osValidationResponse": "readable",
    "osDialogResponse": "readable"
  },
  "rules": {
    "array-bracket-spacing": [
      "warn",
      "never"
    ],
    "arrow-spacing": [
      "warn",
      {
        "before": true,
        "after": true
      }
    ],
    "block-spacing": [
      "warn",
      "always"
    ],
    "brace-style": [
      "warn",
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "comma-dangle": [
      "warn",
      {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "never"
      }
    ],
    "comma-spacing": "warn",
    "comma-style": [
      "warn",
      "last"
    ],
    "computed-property-spacing": [
      "warn",
      "never",
      {
        "enforceForClassMembers": true
      }
    ],
    "dot-location": [
      "warn",
      "property"
    ],
    "eol-last": 0,
    "func-call-spacing": [
      "warn",
      "never"
    ],
    "generator-star-spacing": [
      "warn",
      {
        "before": true,
        "after": true
      }
    ],
    "indent": [
      "warn",
      "tab",
      {
        "SwitchCase": 1,
        "VariableDeclarator": 1,
        "outerIIFEBody": 1,
        "MemberExpression": 1,
        "FunctionDeclaration": {
          "parameters": 1,
          "body": 1
        },
        "FunctionExpression": {
          "parameters": 1,
          "body": 1
        },
        "CallExpression": {
          "arguments": 1
        },
        "ArrayExpression": 1,
        "ObjectExpression": 1,
        "ImportDeclaration": 1,
        "flatTernaryExpressions": false,
        "ignoreComments": false,
        "ignoredNodes": [
          "TemplateLiteral *",
          "JSXElement",
          "JSXElement > *",
          "JSXAttribute",
          "JSXIdentifier",
          "JSXNamespacedName",
          "JSXMemberExpression",
          "JSXSpreadAttribute",
          "JSXExpressionContainer",
          "JSXOpeningElement",
          "JSXClosingElement",
          "JSXFragment",
          "JSXOpeningFragment",
          "JSXClosingFragment",
          "JSXText",
          "JSXEmptyExpression",
          "JSXSpreadChild"
        ],
        "offsetTernaryExpressions": true
      }
    ],
    "key-spacing": [
      "warn",
      {
        "align": "value"
      }
    ],
    "keyword-spacing": [
      "warn",
      {
        "overrides": {
          "if": {
            "after": true
          },
          "else": {
            "after": true,
            "before": true
          },
          "for": {
            "after": true,
            "before": false
          },
          "while": {
            "after": true,
            "before": false
          }
        }
      }
    ],
    "lines-between-class-members": [
      "warn",
      "always",
      {
        "exceptAfterSingleLine": true
      }
    ],
    "multiline-ternary": [
      "warn",
      "always-multiline"
    ],
    "new-parens": "warn",
    "no-extra-parens": [
      "warn",
      "functions"
    ],
    "no-floating-decimal": "warn",
    "no-mixed-operators": [
      "warn",
      {
        "groups": [
          [
            "==",
            "!=",
            "===",
            "!==",
            ">",
            ">=",
            "<",
            "<="
          ],
          [
            "&&",
            "||"
          ],
          [
            "in",
            "instanceof"
          ]
        ],
        "allowSamePrecedence": true
      }
    ],
    "no-mixed-spaces-and-tabs": [
      "warn",
      "smart-tabs"
    ],
    "no-multi-spaces": [
      "warn",
      {
        "ignoreEOLComments": true
      }
    ],
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 1,
        "maxBOF": 0,
        "maxEOF": 0
      }
    ],
    "no-tabs": 0,
    "no-trailing-spaces": "warn",
    "no-whitespace-before-property": "warn",
    "object-curly-newline": [
      "warn",
      {
        "multiline": true,
        "consistent": true
      }
    ],
    "object-curly-spacing": [
      "warn",
      "never"
    ],
    "object-property-newline": [
      "warn",
      {
        "allowMultiplePropertiesPerLine": true
      }
    ],
    "operator-linebreak": [
      "warn",
      "after",
      {
        "overrides": {
          "?": "before",
          ":": "before",
          "|>": "before"
        }
      }
    ],
    "padded-blocks": [
      "warn",
      {
        "blocks": "never",
        "switches": "never",
        "classes": "never"
      }
    ],
    "quote-props": [
      "warn",
      "consistent-as-needed",
      {
        "keywords": true
      }
    ],
    "quotes": [
      "warn",
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": false
      }
    ],
    "rest-spread-spacing": [
      "warn",
      "never"
    ],
    "semi": [
      "warn",
      "always"
    ],
    "semi-spacing": "warn",
    "space-before-blocks": [
      "warn",
      "always"
    ],
    "space-before-function-paren": [
      "warn",
      "never"
    ],
    "space-in-parens": [
      "warn",
      "never"
    ],
    "space-infix-ops": "warn",
    "space-unary-ops": [
      "warn",
      {
        "words": true,
        "nonwords": false
      }
    ],
    "spaced-comment": [
      "warn",
      "always",
      {
        "line": {
          "markers": [
            "*package",
            "!",
            "/",
            ",",
            "="
          ]
        },
        "block": {
          "balanced": true,
          "markers": [
            "*package",
            "!",
            ",",
            ":",
            "::",
            "flow-include"
          ],
          "exceptions": [
            "*"
          ]
        }
      }
    ],
    "template-curly-spacing": [
      "warn",
      "never"
    ],
    "template-tag-spacing": [
      "warn",
      "never"
    ],
    "wrap-iife": [
      "warn",
      "any",
      {
        "functionPrototypeMethods": true
      }
    ],
    "yield-star-spacing": [
      "warn",
      "both"
    ],
    "no-var": "warn",
    "object-shorthand": [
      "warn",
      "never"
    ],
    "accessor-pairs": [
      "warn",
      {
        "setWithoutGet": true,
        "enforceForClassMembers": true
      }
    ],
    "array-callback-return": [
      "warn",
      {
        "allowImplicit": false,
        "checkForEach": false
      }
    ],
    "camelcase": [
      "warn",
      {
        "allow": [
          "^UNSAFE_"
        ],
        "properties": "never",
        "ignoreGlobals": true
      }
    ],
    "constructor-super": "warn",
    "curly": [
      "warn",
      "multi-line"
    ],
    "default-case-last": "warn",
    "dot-notation": [
      "warn",
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [
      "off",
      "always",
      {
        "null": "ignore"
      }
    ],
    "new-cap": [
      "warn",
      {
        "newIsCap": false,
        "capIsNew": false,
        "properties": true
      }
    ],
    "no-array-constructor": "warn",
    "no-async-promise-executor": "warn",
    "no-caller": "warn",
    "no-case-declarations": "warn",
    "no-class-assign": "warn",
    "no-compare-neg-zero": "warn",
    "no-cond-assign": "warn",
    "no-const-assign": "warn",
    "no-constant-condition": [
      "warn",
      {
        "checkLoops": false
      }
    ],
    "no-control-regex": "warn",
    "no-debugger": "warn",
    "no-delete-var": "warn",
    "no-dupe-args": "warn",
    "no-dupe-class-members": "warn",
    "no-dupe-keys": "warn",
    "no-duplicate-case": "warn",
    "no-useless-backreference": "warn",
    "no-empty": [
      "warn",
      {
        "allowEmptyCatch": true
      }
    ],
    "no-empty-character-class": "warn",
    "no-empty-pattern": "warn",
    "no-eval": "warn",
    "no-ex-assign": "warn",
    "no-extend-native": "warn",
    "no-extra-bind": "warn",
    "no-extra-boolean-cast": "warn",
    "no-fallthrough": [
      "warn",
      {
        "allowEmptyCase": true,
        "commentPattern": "break[\\s\\w]*omitted"
      }
    ],
    "no-func-assign": "warn",
    "no-global-assign": "warn",
    "no-implied-eval": "warn",
    "no-import-assign": "warn",
    "no-invalid-regexp": "warn",
    "no-irregular-whitespace": "warn",
    "no-iterator": "warn",
    "no-labels": [
      "warn",
      {
        "allowLoop": false,
        "allowSwitch": false
      }
    ],
    "no-lone-blocks": "warn",
    "no-loss-of-precision": "warn",
    "no-misleading-character-class": "warn",
    "no-prototype-builtins": "warn",
    "no-useless-catch": "warn",
    "no-multi-str": "warn",
    "no-new": "warn",
    "no-new-func": "warn",
    "no-new-object": "warn",
    "no-new-symbol": "warn",
    "no-new-wrappers": "warn",
    "no-obj-calls": "warn",
    "no-octal": "warn",
    "no-octal-escape": "warn",
    "no-proto": "warn",
    "no-redeclare": [
      "warn",
      {
        "builtinGlobals": false
      }
    ],
    "no-regex-spaces": "warn",
    "no-return-assign": [
      "warn",
      "except-parens"
    ],
    "no-self-assign": [
      "warn",
      {
        "props": true
      }
    ],
    "no-self-compare": "warn",
    "no-sequences": "warn",
    "no-shadow-restricted-names": "warn",
    "no-sparse-arrays": "warn",
    "no-template-curly-in-string": "warn",
    "no-this-before-super": "warn",
    "no-throw-literal": "warn",
    "no-undef": "warn",
    "no-undef-init": "warn",
    "no-unexpected-multiline": "warn",
    "no-unmodified-loop-condition": "warn",
    "no-unneeded-ternary": [
      "warn",
      {
        "defaultAssignment": false
      }
    ],
    "no-unreachable": "warn",
    "no-unreachable-loop": "warn",
    "no-unsafe-finally": "warn",
    "no-unsafe-negation": "warn",
    "no-unused-expressions": [
      "warn",
      {
        "allowShortCircuit": true,
        "allowTernary": true,
        "allowTaggedTemplates": true
      }
    ],
    "no-unused-vars": [
      "warn",
      {
        "args": "none",
        "caughtErrors": "none",
        "ignoreRestSiblings": true,
        "vars": "all"
      }
    ],
    "no-use-before-define": [
      "warn",
      {
        "functions": false,
        "classes": false,
        "variables": false
      }
    ],
    "no-useless-call": "warn",
    "no-useless-computed-key": "warn",
    "no-useless-constructor": "warn",
    "no-useless-escape": "warn",
    "no-useless-rename": "warn",
    "no-useless-return": "warn",
    "no-void": "warn",
    "no-with": "warn",
    "one-var": [
      "warn",
      {
        "initialized": "never"
      }
    ],
    "prefer-const": [
      "warn",
      {
        "destructuring": "all"
      }
    ],
    "prefer-promise-reject-errors": "warn",
    "prefer-regex-literals": [
      "warn",
      {
        "disallowRedundantWrapping": true
      }
    ],
    "symbol-description": "warn",
    "unicode-bom": [
      "warn",
      "never"
    ],
    "use-isnan": [
      "warn",
      {
        "enforceForSwitchCase": true,
        "enforceForIndexOf": true
      }
    ],
    "valid-typeof": [
      "warn",
      {
        "requireStringLiterals": true
      }
    ],
    "yoda": [
      "warn",
      "never"
    ]
  }
}